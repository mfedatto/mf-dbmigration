{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "Masking": {
        "YamlEntryConnectionStringRegex":
        {
            "Match": "(connection-string\\s*:\\s*)(.*)$",
            "Replace": "$1$2"
        },
        "ConnectionStringValueRegex": [
            {
                "Match": "(;Password=)[^;]*(;)",
                "Replace": "$1*****$2"
            }
        ]
    }
}
